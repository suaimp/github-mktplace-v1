{
  "name": "tailadmin-react",
  "private": true,
  "version": "2.0.1",
  "type": "module",
  "homepage": "https://suaimp.github.io/github-mktplace-v1/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:notifications": "jest src/db-service/order-notifications",
    "test:services": "jest src/services/db-services/marketplace-services/order",
    "test:header-footer": "jest src/pages/Settings/__tests__",
    "test:header-footer:watch": "jest src/pages/Settings/__tests__ --watch",
    "test:header-footer:coverage": "jest src/pages/Settings/__tests__ --coverage",
    "test:checkout": "node scripts/test-checkout.js",
    "test:checkout:utils": "jest src/utils/__tests__",
    "test:checkout:services": "jest src/services/db-services/marketplace-services/checkout",
    "test:checkout:hooks": "jest src/components/Checkout/hooks/__tests__",
    "test:checkout:components": "jest src/components/Checkout/__tests__",
    "test:checkout:resume-table": "jest src/components/Checkout/__tests__/ResumeTable.test.tsx src/components/Checkout/__tests__/useResumeTableLogic.test.tsx src/components/Checkout/services/__tests__/ResumeTableCacheService.test.ts",
    "test:checkout:pix": "jest src/pages/Checkout/Payment/pix",
    "test:checkout:integration": "jest src/components/Checkout/__tests__/integration",
    "test:checkout:watch": "jest --watch src/components/Checkout",
    "test:checkout:coverage": "jest --coverage src/components/Checkout src/pages/Checkout src/utils",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:sync": "powershell -ExecutionPolicy Bypass -File sync-production-db.ps1",
    "supabase:diff": "supabase db diff",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "env:local": "powershell -Command \"if (Test-Path .env) { Move-Item .env .env.backup -Force }; Write-Host '✅ Configurado para DESENVOLVIMENTO LOCAL'\"",
    "env:prod": "powershell -Command \"if (Test-Path .env.backup) { Move-Item .env.backup .env -Force }; Write-Host '✅ Configurado para PRODUÇÃO'\"",
    "dev:local": "npm run env:local && supabase start && npm run dev",
    "dev:prod": "npm run env:prod && npm run dev"
  },
